{
    "/GlobalEnv/": {
        "C_LOG_LEVEL": "INFO",
        "ETCD_KEEPER_PORT": "7070",
        "GO_LOG_LEVEL": "INFO",
        "GO_VERBOSE": "0",
        "PY_LOG_LEVEL": "INFO"
    },
    "/VideoIngestion/cert_type": [
        "zmq"
    ],
    "/VideoIngestion/config": {
        "encoding": {
            "level": 95,
            "type": "jpeg"
        },
        "ingestor": {
            "loop_video": true,
            "pipeline": "./test_videos/pcb_d2000.avi",
            "poll_interval": 0.2,
            "queue_size": 10,
            "type": "opencv"
        },
        "max_workers": 4,
        "sw_trigger": {
            "init_state": "running"
        },
        "udfs": [
            {
                "n_left_px": 1000,
                "n_right_px": 1000,
                "n_total_px": 300000,
                "name": "pcb.pcb_filter",
                "scale_ratio": 4,
                "training_mode": "false",
                "type": "python"
            }
        ]
    },
    "/VideoIngestion/interfaces": {
        "Publishers": [
            {
                "AllowedClients": [
                    "VideoAnalytics",
                    "Visualizer",
                    "WebVisualizer",
                    "TLSRemoteAgent",
                    "RestDataExport"
                ],
                "EndPoint": "/EII/sockets",
                "Name": "default",
                "Topics": [
                    "camera1_stream"
                ],
                "Type": "zmq_ipc"
            }
        ],
        "Servers": [
            {
                "AllowedClients": [
                    "*"
                ],
                "EndPoint": "0.0.0.0:64013",
                "Name": "default",
                "Type": "zmq_tcp"
            }
        ]
    }
}