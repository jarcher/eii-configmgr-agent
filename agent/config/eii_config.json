    "/Visualizer/interfaces": {
        "Subscribers": [
            {
                "EndPoint": "ia_video_analytics:65013",
                "Name": "default",
                "PublisherAppName": "VideoAnalytics",
                "Topics": [
                    "camera1_stream_results"
                ],
                "Type": "zmq_tcp"
            }
        ]
    },
    "/WebVisualizer/config": {
        "cert_type": ["zmq","pem"],
        "dev_port": 5001,
        "draw_results": "true",
        "labels": {
            "camera1_stream_results": {
                "0": "MISSING",
